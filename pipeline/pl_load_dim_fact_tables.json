					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "\ndelete from stage.dim_customer;",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "customer_id",
