{
	"name": "DataFlowActivies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_storageacct_stage",
						"type": "DatasetReference"
					},
					"name": "LoadTransactionCSV"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_store",
						"type": "DatasetReference"
					},
					"name": "DimStore"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_store",
						"type": "DatasetReference"
					},
					"name": "SinkInsertDimStore"
				}
			],
			"transformations": [
				{
					"name": "DistinctStores"
				},
				{
					"name": "NonExistsStores"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cust_id as string,",
				"          prod_subcat_code as string,",
				"          prod_cat_code as string,",
				"          qty as string,",
				"          rate as string,",
				"          tax as string,",
				"          total_amt as string,",
				"          store_type as string,",
				"          tran_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> LoadTransactionCSV",
				"source(output(",
				"          store_key as integer,",
				"          store_type as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> DimStore",
				"LoadTransactionCSV aggregate(groupBy(store_type),",
				"     count = count(1)) ~> DistinctStores",
				"DistinctStores, DimStore exists(DistinctStores@store_type == DimStore@store_type,",
				"     negate:true,",
				"     broadcast: 'auto')~> NonExistsStores",
				"NonExistsStores sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          store_key as integer,",
				"          store_type as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          store_type",
				"     )) ~> SinkInsertDimStore"
			]
		}
	}
}